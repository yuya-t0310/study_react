  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:api\"",
    "dev:api": "npx json-server -w ./db/db.json -p 3003 -h 127.0.0.1 localhost",
